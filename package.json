{
  "name": "coupon-clicker",
  "version": "1.2.1",
  "description": "Clicks all coupons for Target.com and Vons.com",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node --files src/main.ts",
    "debug": "cross-env NODE_ENV=development ts-node --files src/main.ts",
    "debug:browser": "cross-env NODE_ENV=development HEADLESS=false ts-node --files src/main.ts",
    "lint": "tsc --skipLibCheck --noEmit && prettier --single-quote --w .",
    "postinstall": "node scripts/install.js"
  },
  "git": "https://github.com/jaredkotoff/coupon-clicker.git",
  "author": "Jared Kotoff",
  "license": "ISC",
  "dependencies": {
    "prompts": "^2.4.2",
    "puppeteer": "^21.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "3.6.8",
    "query-selector-shadow-dom": "^1.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
